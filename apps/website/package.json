{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@next/font": "^13.1.1",
    "@portabletext/react": "^2.0.0",
    "@sanity/client": "^3.4.1",
    "next": "13.1.1",
    "next-components": "workspace:*",
    "next-radix": "workspace:*",
    "node-environment": "workspace:*",
    "react": "18.2.0",
    "react-components": "workspace:*",
    "react-dom": "18.2.0",
    "react-hooks": "workspace:*",
    "util-typescript": "workspace:*"
  },
  "devDependencies": {
    "@portabletext/types": "^2.0.0",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "^18.0.10",
    "eslint-config-ethang": "workspace:*",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "tsconfig": "workspace:*",
    "typescript": "4.9.4"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/lib/"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "./test/jestsetup.js"
    ]
  }
}
